{
  "name": "shenyan-utils",
  "type": "module",
  "packageManager": "pnpm@7.33.7",
  "description": "由逻辑开发和业务引导的开源工具函数库",
  "author": "dushenyan <dushenyan88@gmail.com>",
  "license": "MIT",
  "homepage": "https://dushenyan.github.io/10w/pages/home/index.html",
  "repository": "https://github.com/dbudaiya/shenyan-utils.git",
  "keywords": [
    "dushenyan-utils",
    "more-functional",
    "time",
    "validate",
    "typescript",
    "is",
    "stronge"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "icon": "https://cdn.jsdelivr.net/npm/simple-icons@v10/icons/vite.svg",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "nr clean && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish",
    "start": "esno src/mock.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "play:dev": "pnpm --filter playground dev",
    "play:build": "pnpm --filter playground build",
    "play:preview": "pnpm --filter playground preview",
    "lint:ui": "npx @eslint/config-inspector"
  },
  "dependencies": {
    "dayjs": "^1.11.18",
    "jsonc-parser": "^3.3.1",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.6.1",
    "@faker-js/faker": "^10.1.0",
    "@types/doctrine": "^0.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.12.0",
    "@vitest/ui": "^4.0.1",
    "bumpp": "^8.2.1",
    "doctrine": "^3.0.0",
    "eslint": "^9.39.1",
    "esno": "^0.16.3",
    "js-cookie": "^3.0.5",
    "pnpm": "^7.33.7",
    "prismjs": "^1.30.0",
    "typescript": "^4.9.5",
    "unbuild": "^0.8.11",
    "vite": "^7.1.11",
    "vitepress": "^2.0.0-alpha.12",
    "vitest": "^4.0.1"
  }
}
